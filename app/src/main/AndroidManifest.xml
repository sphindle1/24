<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.william.a24">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".game"
            android:parentActivityName=".SelectMode" />
        <activity
            android:name=".SelectMode"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SingleMode"
            android:parentActivityName=".SelectMode" />
        <activity
            android:name=".Tester"
            android:parentActivityName=".SelectMode" />
        <activity
            android:name=".SingleModeHard"
            android:parentActivityName=".SelectMode" />
        <activity
            android:name=".TimeAttackEasy"
            android:parentActivityName=".SelectMode"/>
        <activity
            android:name=".TimeAttackHard"
            android:parentActivityName=".SelectMode"></activity>
    </application>

</manifest>